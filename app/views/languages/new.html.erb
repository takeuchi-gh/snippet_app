<div class="wrapper">
  <%= render partial: "shared/sidebar" %>
  <div class="main">
    <%= form_with(model: @language, local: true) do |form| %>
      <div class="form-group">
        <p>カテゴリー</p>
        <%= form.text_field :name, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= form.submit "登録", class: 'btn btn-primary' %>
      </div>
    <% end %>
  </div>
</div>